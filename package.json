{
  "name": "@uupaa/geohex",
  "version": "0.0.3",
  "description": "GeoHex v3.2 for TypeScript",
  "keywords": [
    "GeoHex",
    "TypeScript",
    "GPS"
  ],
  "main": "index.js",
  "rollup": {
    "input": "dist/GeoHex.js",
    "output": {
      "esm": {
        "file": "dist/GeoHex.esm.js"
      },
      "es5": {
        "file": "dist/GeoHex.es5.js",
        "name": "GeoHexLib"
      },
      "cjs": {
        "file": "dist/GeoHex.cjs.js"
      }
    }
  },
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "bundle": "npm run bundle:esm",
    "bundle:esm": "rollup -c .rollup.esm.js",
    "bundle:es5": "rollup -c .rollup.es5.js",
    "bundle:cjs": "rollup -c .rollup.cjs.js",
    "bundle:all": "npm run bundle:esm; npm run bundle:es5; npm run bundle:cjs",
    "watch": "rollup -w -c .rollup.esm.js &",
    "test": "todo document"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uupaa/GeoHex.git"
  },
  "author": "uupaa.js@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uupaa/GeoHex/issues"
  },
  "homepage": "https://github.com/uupaa/GeoHex#readme",
  "devDependencies": {
    "babel-eslint": "^8.2.5",
    "babel-preset-es2015-rollup": "^3.0.0",
    "eslint": "^5.0.1",
    "npm-run-all": "^4.1.3",
    "request": "",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.5",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-cleanup": "^3.0.0-beta.1",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-license": "^0.12.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-watch": "^4.3.1"
  },
  "unused_devDependencies": {
    "execa": "^0.10.0"
  }
}
